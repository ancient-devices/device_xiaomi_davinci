<resources>

    <!-- make sure you enable this only if your device supports deep slepp
    with active proximity sensor event listener -->
    <bool name="config_proxiSensorWakupCheck">true</bool>

    <!-- Whether to show a custom view for FOD -->
    <bool name="config_needCustomFODView">true</bool>

    <!-- Default fod pressed color -->
    <integer name="config_fod_pressed_color">1</integer>

    <!-- Enable face auth only when swiping security view -->
    <bool name="config_faceAuthOnlyOnSecurityView">true</bool>

    <!-- Whether device supports an alt. ambient display -->
    <bool name="config_alt_ambient_display">true</bool>

    <!-- Whether to enable doze always on -->
    <bool name="config_enableDozeAlwaysOn">true</bool>

    <!-- Enable the RingtonePickerActivity in 'com.android.providers.media'. -->
    <bool name="config_defaultRingtonePickerEnabled">true</bool>

</resources>
